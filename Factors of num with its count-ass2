//PROGRAM ON FACTORS OF A NUMBER AND ITS COUNT//
#include<stdio.h>
#include<conio.h>
main()
{
	int n,i,count=0,totalcount;
	printf("Enter n value:");
	scanf("%d",&n);
	printf("\nFactors of %d are:",n);
	for(i=1;i<=n;i++)
	{
	if(n%i==0)
	{
		printf("%d\t",i);
	count=count+1;
}
}
printf("\n\nTotal factors count= %d",count);
return 0;
}
	
